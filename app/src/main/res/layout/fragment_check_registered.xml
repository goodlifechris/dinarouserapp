<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragments.CheckRegisteredFragment">

    <Button
        android:text="@string/is_registered"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/is_registered_btn"
        app:layout_constraintRight_toLeftOf="parent"
        app:layout_constraintLeft_toRightOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/not_registered_btn"
        app:layout_constraintTop_toTopOf="parent"
        />

    <Button
        android:text="@string/not_registered"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/not_registered_btn"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toLeftOf="parent"
        app:layout_constraintLeft_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        />

</androidx.constraintlayout.widget.ConstraintLayout>